package application;

public class Rules {
	int rows;
	int column;
	int hight;

	int getRows() {
		return rows;
	}

	void putRows(int r) {
		rows = r;
	}

	int getColumn() {
		return column;
	}

	void putColumn(int c) {
		column = c;
	}

	int getHight() {
		return hight;
	}

	void putHight(int h) {
		hight = h;
	}

}
